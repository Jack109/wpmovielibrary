#: ../includes/config/wpmoly-options.php:1573
msgid "TheMovieDB API settings"
msgstr "Réglages de l'API TheMovieDB"

#: ../includes/config/wpmoly-options.php:1580
msgid "Personnal API Key"
msgstr "Clé d'API personnelle"

#: ../includes/config/wpmoly-options.php:1581
msgid "Optional: use your own TMDb API key"
msgstr "Optionnel : utiliser votre propre clé TMDb"

#: ../includes/config/wpmoly-options.php:1582
